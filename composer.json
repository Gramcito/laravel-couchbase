{
    "name": "gramcito/laravel-couchbase",
    "description": "A Couchbase based Eloquent model and Query builder for Laravel",
    "keywords": [
        "laravel",
        "eloquent",
        "couchbase",
        "database",
        "model"
    ],
    "homepage": "https://github.com/ORT-Interactive-GmbH/laravel-couchbase",
    "authors": [],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "illuminate/support": "^6.0",
        "illuminate/container": "^6.0",
        "illuminate/database": "^6.0",
        "illuminate/events": "^6.0",
        "ext-couchbase": ">=2.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^3.9.0",
        "mockery/mockery": "^1.3.1",
      "ext-json": "*"
    },
    "autoload": {
        "psr-0": {
            "Mpociot\\Couchbase": "src/",
            "Mpociot\\Eloquent": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/models",
            "tests/seeds"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mpociot\\Couchbase\\CouchbaseServiceProvider"
            ]
        }
    }
}